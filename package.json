{
  "name": "13vplus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack && npm run compress && npm run optimize",
    "build:analyze": "ANALYZE=true npm run build",
    "build:fast": "next build --turbopack",
    "compress": "gzip -k -f .next/static/**/*.js .next/static/**/*.css || true",
    "optimize": "echo 'Build optimized for production' && du -sh .next/static",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "create-db": "ts-node --project tsconfig.scripts.json scripts/create-database.ts",
    "create-db:simple": "./scripts/create-db.sh",
    "setup-db": "ts-node --project tsconfig.scripts.json scripts/setup-database.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "add-test-products": "ts-node --project tsconfig.scripts.json scripts/add-test-products.ts",
    "check-vitals": "echo 'Run lighthouse audit after deployment'"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "apexcharts": "^5.3.5",
    "flatpickr": "^4.6.13",
    "next": "15.5.3",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-slick": "^0.29.0",
    "sharp": "^0.33.5",
    "slick-carousel": "1.8.1",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "file-loader": "^6.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.3"
  }
}
